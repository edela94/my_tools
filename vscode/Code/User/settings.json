    //     "overviewRulerColor": "#fdf6e3",
    //     "cursor": "pointer",
    //     // "border": "1px solid #eee",
    //     // "borderRadius": "2px",
    //     "isWholeLine": true,
    //     //other styling properties goes here ...
    // },
    // "todohighlight.exclude": [
    //     "**/node_modules/**",
    //     "**/bower_components/**",
    //     "**/dist/**",
    //     "**/build/**",
    //     "**/.vscode/**",
    //     "**/.github/**",
    //     "**/_output/**",
    //     "**/*.min.*",
    //     "**/*.map",
    //     "**/.next/**",
    //     "**/settings.json",
    //     "**/*.i",
    // ],
    // // Enable or disable the highlight
    // "todohighlight.isEnable": true,

    // Removes red error squiggles from source code
    // Usually it's from vscode not knowing the build options
    "C_Cpp.errorSquiggles": "Disabled",

    // Disables grayed out regions from #define
    "C_Cpp.dimInactiveRegions": true,

    // Makes this extension stop taking up constant CPU
    "C_Cpp.intelliSenseCacheSize": 0,

    // Color preferences/themes
    "workbench.sideBar.location": "left",

    // Presentation mode settings
    // Ctrl+Shift+P "Presentation mode"
    // Ctrl+Shift+P "Developer: Toggle screencast mode"
    // "presentationMode.zoomLevel": 2,

    // Enables IAR intellisense and build capabilities
    // "iar.enabled": false,
    "C_Cpp.updateChannel": "Insiders",

    // "atlascode.jira.workingSite": {
