{
    // // Suppress some git warnings
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true
    },

    // Git Bash in VSCode terminal
    // "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",

    // Cygwin, with chere package installed in VSCode terminal
    // "terminal.integrated.shell.windows": "C:\\cygwin\\bin\\bash.exe",
    // "terminal.integrated.shellArgs.windows": ["/bin/xhere", "/bin/bash"],
    // "terminal.integrated.shellArgs.windows": ["--login", "-i"],
    // "terminal.integrated.cwd": "workspaceRoot",

    // Shows the full file path andf root dfirectory at the top
    "window.title": "${activeEditorLong}${separator}${rootName}",

    // Shows the bottom status bar
    "workbench.statusBar.visible": true,

    "window.menuBarVisibility": "default",

    // Trim trailing whitespace
    "files.trimTrailingWhitespace": true,

    // Set default line endings
    "files.eol": "\n",

    // The number of spaces a tab is equal to. This setting is overridden
    // based on the file contents when `editor.detectIndentation` is true.
    "editor.tabSize": 4,

    // Insert spaces when pressing Tab. This setting is overriden
    // based on the file contents when `editor.detectIndentation` is true.
    "editor.insertSpaces": true,

    "workbench.editor.showTabs": true,

    // When opening a file, `editor.tabSize` and `editor.insertSpaces`
    // will be detected based on the file contents. Set to false to keep
    // the values you've explicitly set, above.
    "editor.detectIndentation": false,

    // allows snippets in comments
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": false
    },

    // Breadcrumbs
    "breadcrumbs.enabled": true,

    // Default window zoom level
    "window.zoomLevel": 0,

    // Vertical rulers
    "editor.rulers": [79,119],

    // Whether auto refreshing is enabled
    // "svn.autorefresh": false,

    // Whether auto refreshing is enabled.
    "git.autorefresh": false,

    // // Regex for TODO highlight
    // "todohighlight.isCaseSensitive": false,
    // // "todohighlight.keywordsPattern": "(//|/\*).*(todo|note)",
    // "todohighlight.defaultStyle": {
    //     "color": "black",
