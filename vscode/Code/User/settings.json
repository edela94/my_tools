    "editor.renderWhitespace": "selection",
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "debug.javascript.usePreview": false,
    "python.showStartPage": false,
    "scm.alwaysShowRepositories": true,

